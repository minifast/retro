<%= f = form_for @changeset, "#",
  id: gettext("%{action}-retro-board-form", action: @action),
  phx_target: @myself,
  phx_submit: "save_retro_board" %>

  <%= label f, @label_name %>
  <%= text_input f, :name %>
  <%= error_tag f, :name %>

  <%= submit @button_label, phx_disable_with: "Saving..." %>
</form>
